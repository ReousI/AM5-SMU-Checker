<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationIcon>am5.ico</ApplicationIcon>
  </PropertyGroup>

<PropertyGroup>
  <PublishSingleFile>true</PublishSingleFile>
  <SelfContained>true</SelfContained>
  <RuntimeIdentifier>win-x64</RuntimeIdentifier>
  <PublishAot>true</PublishAot>

  <InvariantGlobalization>true</InvariantGlobalization>
  <DebugType>none</DebugType>
  <StripSymbols>true</StripSymbols>
</PropertyGroup>
  

<PropertyGroup>
    <AssemblyTitle>AM5 SMU Checker</AssemblyTitle>
    <Description>Tool to read SMU from AM5 UEFI files</Description>
    <Product>AM5SMUC</Product>

    <FileVersion>0.0.0.0</FileVersion> 
    <AssemblyVersion>1.0.0.0</AssemblyVersion>  <!-- .NET-Assembly-Version -->
    <InformationalVersion>1.1.6</InformationalVersion>
</PropertyGroup>





  <ItemGroup>
    <PackageReference Include="SharpCompress" Version="0.41.0" />
  </ItemGroup>



</Project>
